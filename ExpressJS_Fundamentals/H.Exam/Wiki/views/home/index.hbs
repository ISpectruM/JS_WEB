<h1>Welcome to SoftUni Wiki!</h1>
{{#data}}
<section id="featured">
    <h2>Latest article</h2>
    <article>
        {{lastArticle.content}}
        <div class="controls">
            <a href="/article/{{lastArticle.id}}">Read more</a>
        </div>
    </article>
</section>
<section id="recent">
    <h2>Recently added articles</h2>
    <ul>
        {{#each lastThreeArticles}}
        <li>
            <a href="/article/{{id}}">{{title}}</a>
        </li>
        {{/each}}
    </ul>
</section>
{{/data}}